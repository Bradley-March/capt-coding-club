  RowBox[{"Clear", "[", "f", "]"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"f", "[", "1", "]"}], "=", "1"}], ";"}], "\n", 
 RowBox[{
  RowBox[{"f", "[", "n_", "]"}], ":=", 
  RowBox[{"n", " ", 
   RowBox[{"f", "[", 
    RowBox[{"n", "-", "1"}], "]"}]}]}]}], "Input",
 CellChangeTimes->{{3.669351025216055*^9, 
  3.669351032346916*^9}},ExpressionUUID->"d414eb23-be66-4f0b-9229-\
f067c69feac2"]
}, Open  ]],

