   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"t", "=", "1"}], ",", "i"}], "}"}], ",", "\[IndentingNewLine]", 
    
    RowBox[{
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"i", "=", "1"}], ",", 
       RowBox[{"i", "\[LessEqual]", "n"}], ",", 
       RowBox[{"i", "++"}], ",", 
       RowBox[{"t", "*=", "i"}]}], "]"}], ";", "\[IndentingNewLine]", "t"}]}],
    "]"}]}]}], "Input",
